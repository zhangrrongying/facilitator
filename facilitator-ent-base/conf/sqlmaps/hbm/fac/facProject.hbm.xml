<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ec.facilitator.base.model.fac.FacProjectModel" table="fac_project">
        <id name="id" type="java.lang.Integer">
            <column name="Id" />
            <generator class="native" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="Name" length="100" not-null="true">
                <comment>项目名称</comment>
            </column>
        </property>
        <property name="projectTypeId" type="java.lang.Integer">
            <column name="Project_Type_Id" not-null="true">
            	<comment>项目类型</comment>
            </column>
        </property>
        <property name="investment" type="java.math.BigDecimal">
            <column name="Investment" precision="18" scale="4" not-null="true">
                <comment>投资规模</comment>
            </column>
        </property>
        <property name="serialNum" type="java.lang.String">
            <column name="Serial_Num" length="50">
                <comment>编号</comment>
            </column>
        </property>
        <property name="description" type="java.lang.String">
            <column name="Description" length="255">
                <comment>描述</comment>
            </column>
        </property>
        <property name="createUser" type="java.lang.Integer">
            <column name="Create_User" />
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="Create_Time" length="19" />
        </property>
        <property name="status" type="java.lang.Short">
            <column name="Status" not-null="true">
                <comment>状态 1：未中标2：已中标3：已完成</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
